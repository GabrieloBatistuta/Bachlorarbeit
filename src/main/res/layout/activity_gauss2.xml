<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="bottom">
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:gravity="center"
        android:layout_weight="1"
        android:onClick="keybordversteken">

        <Button
            android:layout_width="@dimen/allgemeineButton_W"
            android:layout_height="@dimen/allgemeineButton_H"
            android:id="@+id/GerstellenOK2"
            android:onClick="gaussVerwaltung"
            android:background="@drawable/ok"
            android:layout_marginRight="50dp" />

        <Button
            android:layout_width="@dimen/allgemeineButton_W"
            android:layout_height="@dimen/allgemeineButton_H"
            android:id="@+id/Gabbrechen2"
            android:textSize="@dimen/vektoren_TextSize"
            android:onClick="gaussVerwaltung"
            android:background="@drawable/abbrechen" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_weight="2"
        android:gravity="center_horizontal"
        android:onClick="keybordversteken">
        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="false"
            android:columnCount="6"
            android:rowCount="3"
            tools:targetApi="ice_cream_sandwich"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true">

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText1"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="1"
                android:layout_row="0"
                android:digits="0123456789-."
                android:ems="10"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText7"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="1"
                android:layout_row="1"
                android:digits="0123456789-."
                android:ems="10"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText2"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="3"
                android:layout_row="0"
                android:digits="0123456789-."
                android:ems="10"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=") X1 + ("
                android:id="@+id/textView3"
                android:layout_row="1"
                android:layout_column="2"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText8"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="3"
                android:layout_row="1"
                android:digits="0123456789-."
                android:ems="10"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText3"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="5"
                android:layout_row="0"
                android:digits="0123456789-."
                android:ems="10"
                android:enabled="true"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=") X2 ="
                android:id="@+id/textView4"
                android:layout_row="1"
                android:layout_column="4"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <EditText
                android:maxLength="6"
                android:selectAllOnFocus="true"
                android:id="@+id/editText9"
                android:layout_width="@dimen/gauss_TextFeld_w2"
                android:layout_height="@dimen/gauss_TextFeld_h"
                android:layout_column="5"
                android:layout_row="1"
                android:digits="0123456789-."
                android:ems="10"
                android:gravity="center"
                android:inputType="numberDecimal|numberSigned"
                android:padding="1dp"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=") X1 + ("
                android:id="@+id/textView"
                android:layout_row="0"
                android:layout_column="2"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=") X2 ="
                android:id="@+id/textView2"
                android:layout_row="0"
                android:layout_column="4"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="("
                android:id="@+id/textView5"
                android:layout_row="0"
                android:layout_column="0"
                android:textSize="@dimen/gauss_TextErklerung2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="("
                android:id="@+id/textView6"
                android:layout_row="1"
                android:layout_column="0"
                android:textSize="@dimen/gauss_TextErklerung2" />

        </GridLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linefarL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:orientation="vertical"
        android:layout_weight="1"
        android:gravity="center"
        android:onClick="keybordversteken">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:orientation="horizontal"
            android:gravity="center"
            android:onClick="keybordversteken">

            <Button
                android:id="@+id/Ginfo2"
                android:layout_width="@dimen/allgemeineButton_W"
                android:layout_height="@dimen/allgemeineButton_H"
                android:background="@drawable/minfo"
                android:layout_marginRight="50dp"
                android:onClick="gaussVerwaltung" />

            <Button
                android:id="@+id/Gzurueck2"
                android:layout_width="@dimen/allgemeineButton_W"
                android:layout_height="@dimen/allgemeineButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/zuruck"
                android:onClick="gaussVerwaltung" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:auto="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.kojar.rp.UebungenMatrizen"
    android:gravity="center_horizontal">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_weight="1"
        android:layout_margin="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:background="#4c4c4c"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/UMstopuhrstarten"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="@dimen/vektoren_Button_w"
                    android:layout_height="@dimen/vektoren_Button_h"
                    android:text="Starten"
                    android:textColor="#4400ff"
                    android:textSize="@dimen/vektoren_TextSize"
                    android:background="#cfcfcf"
                    android:onClick="stoppuhrVerwalten" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <Chronometer
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/stoppuhrMatrizen"
                    android:textSize="20dp"
                    android:textColor="#1aff00" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/UMstopuhrstoppen"
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="@dimen/vektoren_Button_w"
                    android:layout_height="@dimen/vektoren_Button_h"
                    android:text="Stoppen"
                    android:textColor="#ff0000"
                    android:textSize="@dimen/vektoren_TextSize"
                    android:background="#cfcfcf"
                    android:onClick="stoppuhrVerwalten" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:layout_weight="1"
            android:orientation="horizontal">

            <WebView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/webViewM" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:orientation="horizontal">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/UMtextZeile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <TextView
                    android:id="@+id/textView72"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="Zeile"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="@dimen/matrixen_TextSize" />


                <SeekBar
                    android:id="@+id/UMzeileSeekBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:indeterminate="false" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/UMspalteText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <TextView
                    android:id="@+id/textView73"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="Spalte"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="@dimen/matrixen_TextSize" />


                <SeekBar
                    android:id="@+id/UMspaltenSeekBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:indeterminate="false" />


            </LinearLayout>
        </LinearLayout>

    </LinearLayout>




    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">







            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">









            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">






            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:gravity="center">
            <Button
                android:id="@+id/UMaddieren"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="a+b"
                android:textSize="@dimen/matrixen_TextSize" />

            <Button
                android:id="@+id/UMmultiplikation"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="A*B"
                android:textSize="@dimen/matrixen_TextSize" />

            <Button
                android:id="@+id/UMsubtrahieren"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="a-b"
                android:textSize="@dimen/matrixen_TextSize" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:gravity="center">
            <Button
                android:textAllCaps="false"
                android:id="@+id/UMmitzahlmultiplizieren"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="a* la"
                android:textSize="@dimen/matrixen_TextSize" />

            <Button
                android:id="@+id/UMinverse"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="a^-1"
                android:textSize="@dimen/matrixen_TextSize" />

            <Button
                android:textAllCaps="false"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:text="det(A)"
                android:background="@drawable/funktionen_buttons"
                android:id="@+id/UMdeterminante"
                android:layout_margin="2dp"
                android:onClick="umVerwaltung"
                android:textSize="@dimen/matrixen_TextSize" />

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_vertical"
            android:gravity="center">

            <Button
                android:id="@+id/UMtransponieren"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="A = A^t"
                android:textSize="@dimen/matrixen_TextSize" />
            <Button
                android:id="@+id/UMsymmetrisch"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/funktionen_buttons"
                android:onClick="umVerwaltung"
                android:text="symm"
                android:textSize="@dimen/matrixen_TextSize" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:gravity="center">


            <Button
                android:id="@+id/UMzeigen"
                android:layout_width="@dimen/M_OperationButton_B"
                android:layout_height="@dimen/M_OperationButton_H"
                android:layout_margin="2dp"
                android:background="@drawable/speichernoeffnenloesung_buttons"
                android:onClick="umVerwaltung"
                android:text="LÃ¶sung"
                android:textSize="@dimen/matrixen_TextSize" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:gravity="center">

            <Button
                android:layout_width="@dimen/KontrollButton_B"
                android:layout_height="@dimen/KontrollButton_H"
                android:id="@+id/UMinfo"
                android:background="@drawable/minfo"
                android:onClick="umVerwaltung"
                android:layout_marginRight="50dp" />
            <Button
                android:id="@+id/UMzurueck"
                android:layout_width="@dimen/KontrollButton_B"
                android:layout_height="@dimen/KontrollButton_H"
                android:layout_margin="5dp"
                android:background="@drawable/zuruck"
                android:onClick="umVerwaltung"
                android:textSize="@dimen/matrixen_TextSize" />


        </LinearLayout>
    </LinearLayout>



</LinearLayout>

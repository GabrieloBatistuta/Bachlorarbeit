<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">


    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:orientation="vertical"
        android:gravity="center_horizontal">

        <LinearLayout
            android:id="@+id/linearL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:orientation="vertical"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VerstellenA"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button

                    android:id="@+id/VerstllenB"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VerstellenC"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VphawEingabe"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaGleichc"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VbGleichc"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VaGleichb"
                    android:layout_width="@dimen/V_EingabeButton_B"
                    android:layout_height="@dimen/V_EingabeButton_H"
                    android:layout_margin="5dp"
                    android:background="@drawable/eingabe_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linear"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:orientation="vertical"
            android:layout_weight="3">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaPlusb"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VaMalb"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaminusB"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VbMinusa"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaKreuzB"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VbKreuzA"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaLaenge"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VbLaenge"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/VaSenkrechtb"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"

                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/Vwinkel"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/VphawMala"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />

                <Button
                    android:id="@+id/VphawMalb"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:gravity="top|center_horizontal"
                    android:onClick="vektorenVerwaltung"
                    android:textAllCaps="false"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/Vlinearabhaengig"
                    android:layout_width="@dimen/V_OperationButton_B"
                    android:layout_height="@dimen/V_OperationButton_H"
                    android:layout_margin="3dp"
                    android:background="@drawable/funktionen_buttons"
                    android:onClick="vektorenVerwaltung"
                    android:textSize="@dimen/vektoren_TextSize" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linefarL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="horizontal"
                android:gravity="center">

                <Button
                    android:id="@+id/Vinfo"
                    android:layout_width="@dimen/KontrollButton_B"
                    android:layout_height="@dimen/KontrollButton_H"
                    android:background="@drawable/minfo"
                    android:layout_marginRight="50dp"
                    android:onClick="vektorenVerwaltung" />

                <Button
                    android:id="@+id/Vzurueck"
                    android:layout_width="@dimen/KontrollButton_B"
                    android:layout_height="@dimen/KontrollButton_H"
                    android:layout_margin="2dp"
                    android:background="@drawable/zuruck"
                    android:onClick="vektorenVerwaltung" />
            </LinearLayout>
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
